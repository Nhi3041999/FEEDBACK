@model FEEDBACK.Models.LOAIKHACHSAN

@{
    ViewBag.Title = "Delete";
}

<h2>Xóa loại khách sạn</h2>

<h3 style="color:red; font-style:italic">Bạn có chắc muốn xóa loại khách sạn này?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <h4>Ghi chú</h4>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GHICHU)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" />

        </div>
    }
</div>
@Html.ActionLink(">>Trở lại trang quản lý khách sạn", "Index")
